<Window x:Class="RSS_Subscriber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSS_Subscriber"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FFBB4444">
    <Grid>
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="7*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser x:Name="Browser" Grid.Column="1"/>
        <ListBox x:Name="List" SelectionChanged="List_SelectionChanged" MouseRightButtonUp="List_MouseRightButtonUp" >
            
        </ListBox>
    </Grid>
</Window>
